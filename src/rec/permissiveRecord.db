recordtype(permissive) {
	include "dbCommon.db" 
	field(LABL,DBF_STRING) {
		prompt("Button Label")
		promptgroup(GUI_DISPLAY)
		pp(TRUE)
		interest(1)
		size(20)
	}
	field(VAL,DBF_USHORT) {
		prompt("Status")
		asl(ASL0)
		pp(TRUE)
	}
	field(OVAL,DBF_USHORT) {
		prompt("Old Status")
		special(SPC_NOMOD)
		interest(3)
	}
	field(WFLG,DBF_USHORT) {
		prompt("Wait Flag")
		pp(TRUE)
	}
	field(OFLG,DBF_USHORT) {
		prompt("Old Flag")
		special(SPC_NOMOD)
		interest(3)
	}
}
