		promptgroup(GUI_PULSE)
		pp(TRUE)
		interest(1)
	}
	field(OPW3,DBF_FLOAT) {
		prompt("Output Pulse Width  3")
		promptgroup(GUI_PULSE)
		pp(TRUE)
		interest(1)
	}
	field(DUT4,DBF_FLOAT) {
